   "source": [
    "from sympy import Function, dsolve, Eq, Derivative, sin, cos\n",
    "from sympy.abc import r\n",
    "T = Function('T')\n",
    "h=20\n",
    "k=40\n",
    "b=5e-3\n",
    "beta = h*2/k*b\n",
    "Ta = 30\n",
    "\n",
    "sol = dsolve(-Derivative(r*Derivative(T(r), r), r) - beta*(T(r)), T(r))\n"
   ]
